
  <%- include('partials/header.ejs') %>
  <h1>Submit an image</h1>
  <form action="/posts" enctype="multipart/form-data" method="post">
    <div class="form-group">
      <label for="post-title">Title</label>
      <input type="text" name="title" id="post-title">
      <label for="post-subtitle">Subtitle</label>
      <input type="text" name="subtitle" id="post-subtitle">
      <label for="content">Content</label>
      <input type="text" name="content" id="content">
      <input type="file" class="form-control-file" name="postImages" multiple>
      <input type="submit" value="Submit images!" class="btn btn-default">
    </div>
    <button></button>
  </form>
<%- include('partials/footer') %>
